{
  "name": "ezbot",
  "version": "1.1.5",
  "description": "ezbot",
  "main": "dist/src/index.js",
  "keywords": [],
  "author": "ezbot",
  "license": "ISC",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "rm -rf dist && tsc",
    "dev": "NODE_ENV=development ts-node-dev --require dotenv/config --respawn --transpile-only --poll ./src/index.ts dotenv_config_path=.env",
