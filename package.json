    "postinstall": "tsc",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "chalk": "^4.1.2",
    "discord-player": "^5.2.2",
    "discord.js": "^13.6.0",
    "mathjs": "^10.4.0",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.1",
    "pretty-ms": "^7.0.1",
    "sodium": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "yt-search": "^2.10.3",
    "ytdl-core": "^4.11.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "console-table-printer": "^2.10.0",
    "dotenv": "14.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "17.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
